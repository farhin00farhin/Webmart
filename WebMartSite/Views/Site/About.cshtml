@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="clearfix"></div>

<!--about page content start -->
<section class="team-overall">
    <div class="container">

        <h2 class="wow fadeInDown" data-wow-delay=".25s">
            We care about our customers' preferences and promise to deliver good customer service and quality products. We love innovation and creating beautiful websites.
        </h2>

        <div class="row team-overall-grids">
            <div class="col-md-6 team-overall-grid-left wow fadeInLeft" data-wow-delay=".5s">
                <figure class="effect-lexi">
                    <img src="images/team.jpg" alt="" class="img-responsive">
                    <figcaption>
                        <h3>Webmart <span>Team</span></h3>
                        <p>Each and every wish of a customer is important to us. <span>We care</span> </p>
                    </figcaption>
                </figure>
            </div>
            <div class="col-md-6 team-overall-grid-right wow fadeInRight" data-wow-delay=".5s">
                <h3>Better ways to create <span>websites</span></h3>
                <p>
                    Webmart has been the choice for thousands of
                    loyal customers over the years. We’re an efficient
                    bunch and we love the people we work with. Based
                    in the heart of Manukau, we’re always working
                    close to you and your needs to give you exactly
                    what you want and sometimes, even a little extra.
                    Our 30 person operation means you’ve got the
                    experience and creativity of  dozens of
                    professionals who really know what they’re doing.
                </p>
            </div>
            <div class="clearfix"> </div>
        </div>
        <div class="row">
            <div class="col-xs-12 top-gap">
                <div class="embed-responsive embed-responsive-16by9 wow flipInY" data-wow-delay=".5s" data-wow-duration="2s">
                    <video class="embed-responsive-item" controls="controls" poster="~/Images/vid.png">
                        <source src='@Url.Action("MyVideo")' type="video/mp4" />
                    </video>
                </div>
            </div>
        </div>
    </div>
</section>
<section class="container-fluid team-members">
    <div class="team">
        <div class="container">
            <h3 class="wow lightSpeedIn">Meet Our <span>Team</span></h3>
            <p class="team-description wow lightSpeedIn" data-wow-delay=".5s">
                Meet the people behind all these beautiful, modern and exceptional websites.
            </p>
            <div class="team-grids">
                <div class="col-md-3 team-grid wow fadeInDown" data-wow-offset="400" data-wow-delay=".25s">
                    <figure class="thumb">
                        <img src="images/4.jpg" alt="team-member -Andrew Anderson" class="img-responsive">
                        <figcaption class="caption">
                            <h3><a href="team.html">Rick Anderson</a></h3>
                            <span>CEO</span>
                            <div class="social-icon">
                                <ul>
                                    <li><a href="#" class="icon-button team-twitter"><i class="icon-twitter"></i><span></span></a></li>
                                    <li><a href="#" class="icon-button team-google"><i class="icon-google"></i><span></span></a></li>
                                    <li><a href="#" class="icon-button team-v"><i class="icon-v"></i><span></span></a></li>
                                </ul>
                            </div>
                        </figcaption>
                    </figure>
                </div>
                <div class="col-md-3 team-grid wow fadeInUp" data-wow-offset="400" data-wow-delay=".25s">
                    <figure class="thumb">
                        <img src="images/3.jpg" alt="team member- Adam Smith" class="img-responsive">
                        <figcaption class="caption">
                            <h3><a href="team.html">Adam Smith</a></h3>
                            <span>Designer</span>
                            <div class="footer-bottom-right social-icon">
                                <ul>
                                    <li><a href="#" class="icon-button team-twitter"><i class="icon-twitter"></i><span></span></a></li>
                                    <li><a href="#" class="icon-button team-google"><i class="icon-google"></i><span></span></a></li>
                                    <li><a href="#" class="icon-button team-v"><i class="icon-v"></i><span></span></a></li>
                                </ul>
                            </div>
                        </figcaption>
                    </figure>
                </div>
                <div class="col-md-3 team-grid wow fadeInDown" data-wow-offset="400" data-wow-delay=".25s">
                    <figure class="thumb">
                        <img src="images/5.jpg" alt="team member - Christ Richard" class="img-responsive">
                        <figcaption class="caption">
                            <h3><a href="team.html">Christ Richard</a></h3>
                            <span>Developer</span>
                            <div class="footer-bottom-right social-icon">
                                <ul>
                                    <li><a href="#" class="icon-button team-twitter"><i class="icon-twitter"></i><span></span></a></li>
                                    <li><a href="#" class="icon-button team-google"><i class="icon-google"></i><span></span></a></li>
                                    <li><a href="#" class="icon-button team-v"><i class="icon-v"></i><span></span></a></li>
                                </ul>
                            </div>
                        </figcaption>
                    </figure>
                </div>
                <div class="col-md-3 team-grid wow fadeInUp" data-wow-offset="400" data-wow-delay=".25s">
                    <figure class="thumb">
                        <img src="images/6.jpg" alt="team member - Rita Singh" class="img-responsive">
                        <figcaption class="caption">
                            <h3><a href="team.html">Rinita Singh</a></h3>
                            <span>Developer</span>
                            <div class="footer-bottom-right social-icon">
                                <ul>
                                    <li><a href="#" class="icon-button team-twitter"><i class="icon-twitter"></i><span></span></a></li>
                                    <li><a href="#" class="icon-button team-google"><i class="icon-google"></i><span></span></a></li>
                                    <li><a href="#" class="icon-button team-v"><i class="icon-v"></i><span></span></a></li>
                                </ul>
                            </div>
                        </figcaption>
                    </figure>
                </div>
                <div class="clearfix"> </div>
            </div>
        </div>
    </div>
</section>
<!--about page content end -->

